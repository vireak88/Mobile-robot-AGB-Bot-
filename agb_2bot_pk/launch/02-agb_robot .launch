<?xml version="1.0" encoding="UTF-8"?>
<launch>



<arg name="model" default="$(find agb_2bot_pk)/urdf/agb_robot.urdf.xacro"/>

<rosparam file="$(find agb_2bot_pk)/YAML/pid.yaml" command="load"/>
<!-- Launch gazebo -->
<include file="$(find agb_2bot_pk)/launch/gazebo.launch">
  <arg name="model" value="$(arg model)"/>
  
 </include>

<!-- Load joint controller configuration from YAML file to parameter server -->

<rosparam file="$(find agb_2bot_pk)/config/joint_states.yaml" command="load"/>

<rosparam file="$(find agb_2bot_pk)/config/agb_robot.yaml" command="load"/>


<node name="robotcontroller_spawner" pkg="agb_2bot_pk" type="example_can.py"/>

<!--node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui"/>
 <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/-->

<node name="rqt_robot_steering" pkg="rqt_robot_steering" type="rqt_robot_steering">
  <param name="default_topic" value="/AGB_Robot/agb_robot_controller/cmd_vel"/> 
</node>

</launch>

 


